# Configuration de l'API Spring Boot
REACT_APP_API_URL=http://localhost:8080

# Configuration pour la production
# REACT_APP_API_URL=https://api.ia-lab.fr

# Configuration SWR (optionnel)
REACT_APP_SWR_REFRESH_INTERVAL=30000
REACT_APP_SWR_DEDUPING_INTERVAL=2000

# Configuration d'authentification (optionnel)
REACT_APP_AUTH_TOKEN_KEY=ia_lab_token
REACT_APP_AUTH_REFRESH_TOKEN_KEY=ia_lab_refresh_token

# Configuration de debug (optionnel)
REACT_APP_DEBUG_SWR=false
REACT_APP_LOG_LEVEL=info

# clé de chiffrement (doit être une chaîne de 32 caractères)
REACT_APP_ENCRYPTION_KEY= "0123456789abcdef0123456789abcdef"